.section-contact .webform-client-form .form-select, #pa-user-workflow-register-form select, #user-profile-form .form-select{display:block;clear:both;width:280px; margin: 0 0 10px 0; padding:9px 5px 9px 13px; color: #969696; background:#f6f6f6;border:1px solid #d5d5d5;border-radius:4px;-moz-border-radius:4px;-webkit-border-radius:4px;-webkit-box-shadow:inset 0px 1px 1px 0px #dddddd;-moz-box-shadow: inset 0px 1px 1px 0px #dddddd;box-shadow: inset 0px 1px 1px 0px #dddddd; box-sizing: content-box;}
