.readmore-btn:hover{ text-decoration:underline}
.hero-itemcol:hover{border:2px solid #eca628;-webkit-transition: all .2s linear;-moz-transition: all .2s linear;-o-transition: all .2s linear;transition: all .2s linear;}
.hero-itemcol:hover .img-overlay{background:#eca628;-webkit-transition: all .2s linear;-moz-transition: all .2s linear;-o-transition: all .2s linear;transition: all .2s linear;}
.hero-itemcol .readmore-btn{margin-top:0;}
