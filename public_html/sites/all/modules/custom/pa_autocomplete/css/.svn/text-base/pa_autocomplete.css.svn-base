.ui-helper-hidden-accessible{
  display:none;
}
ul.ui-autocomplete{
  list-style:none;
  list-style-image:none;
  margin:0;
  padding:0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
  clear:both;
}
ul.ui-autocomplete li{
  padding: 5px;
  background: none repeat scroll 0 0 #ECA628;
  border: 1px solid #fff;
  border-top:none;
  position: relative;
  margin: 0;
  min-height: 0;
  cursor: pointer;
}
ul.ui-autocomplete li a, ul.ui-autocomplete li a:visited{
  color:#fff;
  font-size:12px;
  cursor:pointer;
  position:relative;
  font: 12px/14px "proxima_nova_rgbold";
}