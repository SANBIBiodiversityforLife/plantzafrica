.img-overlay{float:left; width:195px;padding: 7px 0 5px 11px;text-align:left; color:#ffffff; font:16px/20px 'proxima_nova_rgbold'; background:#fcb535; position:absolute; left:0; top:0; z-index:30; overflow:hidden;-webkit-transition: all .2s linear;-moz-transition: all .2s linear;-o-transition: all .2s linear;transition: all .2s linear; margin:8px 0 0 8px;}
